<?xml version="1.0" encoding="utf-8"?>
<Application>
  <Views>
    <DetailView Id="LookUpListSearch_DetailView">
      <Items>
        <PropertyEditor Id="Customer" View="LookUpListSearch_LookupListView" />
      </Items>
      <Layout>
        <LayoutGroup Id="Main" RelativeSize="100">
          <LayoutGroup Id="SimpleEditors" RelativeSize="100">
            <LayoutGroup Id="Order" RelativeSize="5">
              <LayoutItem Id="Customer" RelativeSize="100" />
            </LayoutGroup>
            <LayoutGroup Id="OrderBase" Direction="Horizontal" RelativeSize="94">
              <LayoutItem Id="OrderDate" Removed="True" />
              <LayoutItem Id="Reference" Removed="True" />
              <LayoutItem Id="Total" Removed="True" />
            </LayoutGroup>
          </LayoutGroup>
          <LayoutGroup Id="OrderLines_Group" Removed="True" />
        </LayoutGroup>
      </Layout>
    </DetailView>
    <ListView Id="LookUpListSearch_LookupListView" LookUpListSearch="AlwaysEnable" />
  </Views>
</Application>